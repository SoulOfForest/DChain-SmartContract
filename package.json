    "test:coverage": "hardhat coverage",
    "test:gas": "cross-env REPORT_GAS=true yarn test",
    "doc:gen": "hardhat dodoc",
    "compile": "npx hardhat compile"
  },
  "lint-staged": {
    "*.sol": [
      "solhint -c .solhint.json",
      "prettier --write",
      "yarn test"
    ],
    "*.md": "prettier --write"
